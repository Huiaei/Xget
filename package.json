{
  "dependencies": {
    "express": "^4.19.2",
    "xget": "file:"
  },
  "devDependencies": {
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "vitest": "^3.1.4"
  },
  "name": "xget",
  "private": false,
  "scripts": {
    "build": "node scripts/build.js",
    "build:static": "node scripts/build.js",
    "deploy": "echo 'Deploy via EdgeOne Pages dashboard or CLI'",
    "dev": "node server.js",
    "format": "prettier --write src/ test/ functions/ *.js *.json *.md",
    "format:check": "prettier --check src/ test/ functions/ *.js *.json *.md",
    "lint": "eslint src/ test/ functions/",
    "lint:fix": "eslint src/ test/ functions/ --fix",
    "start": "npm run dev",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit"
  },
  "type": "module",
  "version": "1.0.0"
}
